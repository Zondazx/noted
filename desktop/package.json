    "dist": "electron-builder -mwl"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "6.0.0",
    "electron-builder": "21.2.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "electron-util": "0.12.1"
  }
}
