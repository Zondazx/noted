{
  "name": "noted-desktop",
  "version": "0.1.0",
  "description": "Electron client for Noted.",
  "main": "src/index.js",
  "build": {
    "appId": "io.noted",
    "productName": "Noted",
    "directories": {
      "buildResources": "resouces"
    }
  },
  "scripts": {
